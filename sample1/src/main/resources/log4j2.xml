<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appenders>
    <Console name="STDOUT" target="system_out">
      <PatternLayout pattern="%d [%thread] %-5p [%c] [%F:%L] - %msg%n"/>
    </Console>
  </appenders>
  <loggers>
    <root level="info">
      <appender-ref ref="STDOUT" />
    </root>
  </loggers>
</configuration>